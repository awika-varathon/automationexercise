    "reviewYourOrderSummaryTotal": {
        "name": "reviewYourOrder",
        "mainElement": "#cart_info table",
        "mappingCheckElementObject": [
            {
                "name": "cartOrderTotal",
                "type": "element",
                "elementName": "tbody tr:last-child",
                "checkElement": [
                    { 
                        "key": "orderSummaryTotal", 
                        "id": "td p.cart_total_price", 
                        "type": "text", 
                        "matchType": "equal", 
